.search-table input[type=checkbox],
.search-table input[type=radio],
.form-table input[type=checkbox],
.form-table input[type=radio] {
    display: none;
    position: absolute;
    left: 2px;
    top: 3px;
    margin: 0;
    z-index: -1;
}

.search-table input[type=checkbox] + span,
.search-table input[type=radio] + span,
.form-table input[type=checkbox] + span,
.form-table input[type=radio] + span {
    position: relative;
    left: 0;
    display: inline-block;
    width: 23px;
    height: 23px;
    background: url(./images/checkbox.gif) no-repeat;
}

.search-table input[type=checkbox] + span,
.search-table input[type=radio] + span,
.form-table input[type=checkbox] + span,
.form-table input[type=radio] + span {
    background-position: -12px -12px;
}

.search-table input[type=checkbox]:checked + span,
.search-table input[type=radio]:checked + span,
.form-table input[type=checkbox]:checked + span,
.form-table input[type=radio]:checked + span {
    background-position: -12px -212px;
}

