package com.zhskg.bag.data;

/**
 * 箱包丢失封装数据
 * Created by lwb on 2018/5/16.
 */
public class CacheData
{
    //手机号码 用于极光推送
    private String phoneNumber;

    private String clientId;
    /**
     * 丢失标志：0是正常，1 是丢失
     * 当丢失标志是1的话：下发死锁命令
     */
    private int lossFlag;

    private String ip;
    /**
     * 下发参数
     * 当用户设置过参数的时候，有数据上来就下发参数配置下去
     * 0是不要下发，1是要下发
     */
    private int paramFlag;

    private int port;

    private String cpu;


    public String getPhoneNumber() {
        return phoneNumber;
    }

    public void setPhoneNumber(String phoneNumber) {
        this.phoneNumber = phoneNumber;
    }

    public String getClientId() {
        return clientId;
    }

    public void setClientId(String clientId) {
        this.clientId = clientId;
    }

    public int getLossFlag() {
        return lossFlag;
    }

    public void setLossFlag(int lossFlag) {
        this.lossFlag = lossFlag;
    }

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }

    public int getParamFlag() {
        return paramFlag;
    }

    public void setParamFlag(int paramFlag) {
        this.paramFlag = paramFlag;
    }

    public int getPort() {
        return port;
    }

    public void setPort(int port) {
        this.port = port;
    }

    public String getCpu() {
        return cpu;
    }

    public void setCpu(String cpu) {
        this.cpu = cpu;
    }
}
